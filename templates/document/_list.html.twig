 <table class="table table-striped table-hover cat_table table-responsive-stack" id="tableOne">
    <thead>
        <tr>
            <th>DATE</th>
            <th>DESCRIPTION</th>
            <th>NOM</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    {% for document in documents %}
        <tr>
            <td>
                {{ document.updatedAt|date("d/m/Y") }}
            </td>
            <td>
                {{ document.description }}
            </td>
            <td>
                {{ document.document }}
            </td>
            <td class="d-flex flex-row align-items-center">
                <a class="button button-light p-2 d-flex text-center me-2" href="{{ path('document_download', {'id': document.id}) }}"><span class="material-icons ps-3">download</span></a>
                {{ include('document/_delete_form.html.twig') }}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>